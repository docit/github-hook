{
    "name": "docit/github-hook",
    "description": "GitHub hook for Docit.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "docit/core": "1.0.*",
        "docit/support": "1.0.*",
        "vierbergenlars/php-semver": "3.0.*",
        "graham-campbell/github": "~4.0",
        "potherca/flysystem-github": "0.2.*"
    },
    "autoload": {
        "psr-4": {
            "Docit\\Hooks\\Github\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
